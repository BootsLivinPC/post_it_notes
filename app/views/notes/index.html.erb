<h1> Post It Notes </h1>
<hr />

<%= link_to "Create New Note", new_note_path %>

<% @notes.each do |note| %>
  <h2><%= note.title %></h2>
  <%= link_to "Show Note", note_path(note.id) %>
  <%= link_to "Edit Note", edit_note_path(note.id) %>
  <%= link_to "Delete Note", note_path(note), method: :destroy %>

<% end %>
